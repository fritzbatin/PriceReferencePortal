@model IEnumerable<PriceReferencePortal.Context.supply>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.vendor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sku)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.delivery_lead_time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.validity)
        </th>
        <th>
            
            @Html.DisplayNameFor(model => model.date_created)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.created_by)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.vendor)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.sku)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.description)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.price)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.delivery_lead_time)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.validity)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.date_created)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.created_by)
    </td>
    <td>

        <input type="button" value="Add" onclick="location.href='@Url.Action("AddOrder", new { id = item.Id})'" />
    </td>
       
</tr>
    }

</table>
